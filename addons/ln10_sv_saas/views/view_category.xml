<odoo>
  <data>
    <record id="categoria_view_form" model="ir.ui.view">
      <field name="name">Categoria de Modulos</field>
      <field name="model">ln10_sv_saas.category</field>
      <field name="arch" type="xml">
          <form string="Categoria de Modulos">
              <sheet>
                  <group >
                     <group>
                     
                          <field name="name"/>
                      
                     </group>
                      <group >
                          <field style="color:black;"   name="git_url"    placeholder="https://github.com/user/ejemplo.git"/>
                    
                      </group>
                      <notebook>
                          <page string="Descripcion">
                              <group>
                                <field name="descripcion"/>
                              </group>
                          </page>
                      </notebook>
                  </group>
              </sheet>
              <div class="oe_chatter">
                <field name="message_follower_ids"/>
                <field name="activity_ids"/>
                <field name="message_ids"/>
            </div>
          </form>
      </field>
  </record>

<record model="ir.ui.view" id="rocket_categoria_list">
<field name="name">Categorias de modulos </field>
<field name="model">ln10_sv_saas.category</field>
<field name="arch" type="xml">
  <tree>
    <field name="name"/>
    <field name="descripcion"/>
    <field name="git_url"/>         
  </tree>
</field>
</record>
  </data>
</odoo>