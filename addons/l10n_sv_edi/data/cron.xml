<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ir_cron_revisar_saldo" model="ir.cron">
            <field name="name">Revisar saldo</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="base.model_res_company"/>
            <field eval="False" name="doall"/>
            <field name="state">code</field>
            <field name="code">model.get_saldo_by_cron()</field>
        </record>

        <record id="ir_cron_revisar_cancelar" model="ir.cron">
            <field name="name">Revisar facturas por cancelar</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field eval="False" name="doall"/>
            <field name="state">code</field>
            <field name="code">model.check_cancel_status_by_cron()</field>
        </record>
         
        <record id="ir_cron_generate_token" model="ir.cron">
            <field name="name">Modo ambiente: Modo de prueba </field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">48</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="base.model_res_company"/>
            <field eval="False" name="doall"/>
            <field name="state">code</field>
            <field name="code">model.validar_token_s_ocha()</field> 
        

        </record>

        <record id="ir_cron_generate_token_produccion" model="ir.cron">
            <field name="name">Modo de ambiente: Modo de produccion </field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">24</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="base.model_res_company"/>
            <field eval="False" name="doall"/>
            <field name="state">code</field>
            <field name="code">model.validar_token_s_ocha()</field>       
        </record>    
          <record id="ir_cron_campos" model="ir.cron">
            <field name="name">Contador DTE Firmas</field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field eval="False" name="doall"/>
            <field name="state">code</field>
            <field name="code">model.actualizar_cp()</field>       
        </record>    
       
       
   </data>
</odoo>

